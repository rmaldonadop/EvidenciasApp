<p id="notice"><%= notice %></p>

<h1>Tipo Evidencia</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_evidencia.each do |tipo_evidencium| %>
      <tr>
        <td><%= tipo_evidencium.codigo %></td>
        <td><%= tipo_evidencium.tipo %></td>
        <td><%= link_to 'Show', tipo_evidencium %></td>
        <td><%= link_to 'Edit', edit_tipo_evidencium_path(tipo_evidencium) %></td>
        <td><%= link_to 'Destroy', tipo_evidencium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Evidencium', new_tipo_evidencium_path %>
